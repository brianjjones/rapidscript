#:kivy 1.4.2

<IfStatement>:
    size: 50, 70
    canvas:
        Color:
            rgba: 1, 0, 0, 1

        Rectangle:
            pos:self.pos
            size:self.size


<Print>:
    size: 50, 50
 
    canvas:
        Color:
            rgba: 1, 2, 0, .8

        Rectangle:
            pos:self.pos
            size:self.size


